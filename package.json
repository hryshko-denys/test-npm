  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hryshko-denys/test-npm.git"
  },
  "keywords": ["Solana"],
  "author": "D-H",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hryshko-denys/test-npm/issues"
  },
  "homepage": "https://github.com/hryshko-denys/test-npm#readme",
  "devDependencies": {
    "prettier": "^2.6.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.6.4"
  },
  "files": ["lib/**/*"]
}
